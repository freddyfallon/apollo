{
  "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "import/no-extraneous-dependencies": [0, {"devDependencies": true }],
      "no-underscore-dangle": 0,
      "prettier/prettier": ["error", {
        "singleQuote": true,
        "trailingComma": "all"
      }],
      "no-unused-expressions": 0,
      "no-debugger": ["warn"]
  }
}
